{
  "remove": [
    "tsup",
    "packageManager"
  ],
  "replace": {
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "main": "dist/index.cjs",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./dist/*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js",
        "require": "./dist/*.cjs"
      },
      "./transformer": {
        "types": "./dist/transformer.d.ts",
        "default": "./dist/transformer.js",
        "require": "./dist/transformer.cjs"
      },
      "./package.json": "./package.json"
    }
  }
}
